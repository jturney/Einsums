{
  "name": "Einsums",
  // "image": "nvcr.io/nvidia/nvhpc:24.5-runtime-cuda12.4-ubuntu22.04",
  "image": "jetturney/test",
  "runArgs": [
    "--gpus=all",
    "--cap-add=SYS_PTRACE",
    "--security-opt",
    "seccomp=unconfined"
  ],
  "remoteUser": "vscode",
  "customizations": {
    "vscode": {
      "settings": {
        "terminal.integrated": {
          "profiles.linux": {
            "zsh": {
              "path": "zsh"
            }
          },
          "defaultProfile.linux": "zsh"
        },
        "files.eol": "\n",
        "resmon.show.battery": false,
        "resmon.show.cpufreq": false
      },
      "extensions": [
        "ms-vscode.cmake-tools",
        "llvm-vs-code-extensions.vscode-clangd",
        "mutantdino.resourcemonitor",
        "matepek.vscode-catch2-test-adapter",
        "RSIP-Vision.nvidia-smi-plus"
      ]
    }
  }
}